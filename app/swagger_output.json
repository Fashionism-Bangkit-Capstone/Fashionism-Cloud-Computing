          {
            "name": "price",
            "in": "formData",
            "type": "string",
            "description": "Product Price (number)",
            "required": true,
            "value": "100000"
          },
          {
            "name": "type_id",
            "in": "formData",
            "type": "string",
            "description": "Product Type (number)",
            "required": true,
            "value": "1"
          },
          {
            "name": "category_id",
            "in": "formData",
            "type": "string",
            "description": "Product Category (number)",
            "required": true,
            "value": "1"
          },
          {
            "name": "product_image",
            "in": "formData",
            "type": "file",
            "description": "Product Image",
            "required": true
          }
        ],
        "responses": {
          "201": {
            "description": "Created"
          },
          "401": {
            "description": "Unauthorized"
